    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Inovaideia.Keycloak.Adapter</PackageId>
    <Version>1.0.0</Version>
    <Title>C# adapter to keycloak</Title>
    <Authors>Eduardo Prudencio</Authors>
    <PackageLicenseExpression>MS-PL</PackageLicenseExpression>
    <PackageRequireLincenseAcceptance>true</PackageRequireLincenseAcceptance>
    <PackageReleaseNotes>Shareding library packaged for nuget</PackageReleaseNotes>
